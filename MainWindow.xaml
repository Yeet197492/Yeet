<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="Chicken Nuggets" Height="350" Width="10000" Cursor="Wait">
    <StackPanel>
        <Label x:Name="lblHello" Content="Hello World" FontSize="40"/>
        <Button x:Name="btnHover" Content="Add Water" Click="btnClick_Click" FontSize="40" Opacity="0.1" Cursor="Wait" >
            <Button.Effect>
                <DropShadowEffect/>
            </Button.Effect>
        </Button>
        <Button x:Name="Yeet_boi" Content="moisturise" Click="btnClick_Click" FontSize="120" Cursor="Wait">
            
        </Button>
    </StackPanel>
</Window>
